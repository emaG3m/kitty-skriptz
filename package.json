{
  "name": "kitty-skriptz",
  "version": "0.1.0",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "build",
    "lib",
    "src"
  ],
  "dependencies": {
    "moment": "^2.18.1",
    "truffle-blockchain-utils": "^0.0.3",
    "truffle-hdwallet-provider": "^0.0.3",
    "yargs": "^10.0.3",
    "zeppelin-solidity": "1.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.0.4",
    "ethdeploy": "^1.1.0",
    "ethdeploy-environment-loader": "^1.0.0",
    "ethjs": "^0.3.0",
    "jest-cli": "^20.0.4",
    "lk-ethdeploy-solc-loader": "0.0.1",
    "lk-test-helpers": "^0.1.3",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  },
  "scripts": {
    "start": "node src/index.js info",
    "build": "solc --allow-paths node_modules/zeppelin-solidity/**/*.sol -o build --bin --abi --overwrite zeppelin-solidity/=node_modules/zeppelin-solidity/ contracts/**",
    "deploy:testrpc": "ethdeploy config/testrpc.config.js",
    "test": "yarn build && scripts/test.sh",
    "testrpc": "scripts/runTestrpc.sh",
    "run-jest": "jest",
    "lint": "standard --verbose | snazzy"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src",
      "test"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/helpers/setup.js"
  }
}
